<gdr:CopyOutputViewRoot x:Class="Gar.Desktop.Views.UrlEncoderView"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                        xmlns:uid="http://gar.katotek.com/ClientDesign"
                        xmlns:gdr="clr-namespace:Gar.Desktop.Root"
                        xmlns:gc="clr-namespace:Gar.Desktop.Controls"
                        mc:Ignorable="d"  
                        d:DataContext="{d:DesignInstance Type=uid:UrlEncoderViewModel, IsDesignTimeCreatable=True}">
    <gc:InputOutputControl Input="{Binding Input}" 
                           Output="{Binding Output, Mode=OneWay}" 
                           ClearInputCommand="{Binding ClearInputCommand}" 
                           CopyOutputCommand="{Binding CopyOutputCommand}"/>
</gdr:CopyOutputViewRoot>
